# API Configuration
APP_NAME=Olive API
APP_DESCRIPTION=FastAPI backend with SQLite and SQLModel
APP_VERSION=1.0.0

# Server Configuration
HOST=0.0.0.0
PORT=8000
RELOAD=true
LOG_LEVEL=info

# CORS Configuration
# Comma-separated list of allowed origins
CORS_ORIGINS=*
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_METHODS=*
CORS_ALLOW_HEADERS=*

# Database Configuration
DATABASE_URL=sqlite:///./olive.db

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
